        <h1>Galeria fotografica del desarrollo del proyecto.<span>A flat-style take on a Polaroid gallery</span></h1>
      </header>
  
  <div>
    <div class="container">

  <tbody>
    <% @imagenes.each do |imagene| %>
    <div class="col-md-4">
        <h2><strong><%= imagene.texto %></strong></h2>
        <div><%= image_tag imagene.image_url(:thumb).to_s %></div>
        <div><%= imagene.description %></div>
        <% if can? :create, Imagene %>
          <div><%= link_to 'Edit', edit_imagene_path(imagene) %></div>
        <% end %>

        <% if can? :delete, Imagene %>
          <div><%= link_to 'Destroy', imagene, method: :delete, data: { confirm: 'Are you sure?' } %></div>
        <% end %>
    </div>

    <% end %>
  </tbody>
</div>

<% if can? :create, Imagene %>
  <%= link_to 'New Image', new_imagene_path %> 
<% end %>

<div class="paginate">
  <div class="container " style="text-align:center;">
    <%= will_paginate @imagenes %>
  </div>
</div>
 
